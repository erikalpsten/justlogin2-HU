<ul>
  <% if ::Refinery::Products::Admin::UserSubmissionsController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.products_admin_user_submissions_path %>
    </li>
  <% end %>
  <li>
    <%#= action_label :add, refinery.new_products_admin_user_submission_path, t('.create_new') %>
  </li>
<% if !searching? && ::Refinery::Products::Admin::UserSubmissionsController.sortable? && ::Refinery::Products::UserSubmission.many? %>
  <li>
    <%= action_label :reorder,
                     refinery.products_admin_user_submissions_path,
                     t('.reorder', what:  "User Submissions"), id: 'reorder_action'%>
    <%= action_label :reorder_done,
                     refinery.products_admin_user_submissions_path,
                      t('.reorder_done', what: "User Submissions"), id: 'reorder_action_done', class: 'hidden'%>
  </li>
<% end %>
</ul>
