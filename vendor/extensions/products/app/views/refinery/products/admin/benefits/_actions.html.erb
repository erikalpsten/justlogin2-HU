<ul>
  <% if ::Refinery::Products::Admin::BenefitsController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.products_admin_benefits_path %>
    </li>
  <% end %>
  <li>
    <%= action_label :add, refinery.new_products_admin_benefit_path, t('.create_new') %>
  </li>
<% if !searching? && ::Refinery::Products::Admin::BenefitsController.sortable? && ::Refinery::Products::Benefit.many? %>
  <li>
    <%= action_label :reorder,
                     refinery.products_admin_benefits_path,
                     t('.reorder', what:  "Benefits"), id: 'reorder_action'%>
    <%= action_label :reorder_done,
                     refinery.products_admin_benefits_path,
                      t('.reorder_done', what: "Benefits"), id: 'reorder_action_done', class: 'hidden'%>
  </li>
<% end %>
</ul>
