<tr class='<%= cycle("on", "on-hover") %>' id="<%= dom_id(user_submission) -%>">
  <td>
    <%= user_submission.first_name %>
  </td>
  <td>
    <%= user_submission.last_name %>
  </td>
  <td>
    <%= user_submission.email %>
  </td>
  <td>
    <%= user_submission.plan_names.try(:join, ' | ') %>
  </td>
  <td>
    <%= user_submission.product_names %>
  </td>
  <td>
    <%= action_icon(:edit, refinery.edit_products_admin_user_submission_path(user_submission), t('.edit')) %>
    <%= action_icon(:delete, refinery.products_admin_user_submission_path(user_submission), t('.delete'),
                    { class: "cancel confirm-delete",
                      data: { confirm: t('message', scope: 'refinery.admin.delete', title: user_submission.first_name) } }) %>
  </td>
</tr>
